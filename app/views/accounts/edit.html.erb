<%= form_for :user, url: account_path(@user), method: :patch do |f| %>

	<%= f.fields_for :profile_attributes, @user.profile do |p| %>
	<div class="form-group">
    <%= p.label :full_name %>
    <%= p.text_field :full_name, :class => "form-control" %>
  </div>
  <% end %>

  <div class="form-group">
    <%= f.submit "Save", :class => "btn btn-primary" %>
    <%= link_to 'Back', account_path, :class => "btn btn-link" %>
  </div>

<% end %>